{
    "name": "nextjs-template-project",
    "version": "0.1.0",
    "private": true,
    "main": "./.next/electron/main.js",
    "scripts": {
        "dev:electron": "cross-env NODE_ENV=development webpack --config webpack.electron.config.js --mode development && electron .",
        "build:electron": "cross-env NODE_ENV=production webpack --config webpack.electron.config.js --mode production",
        "dev:next": "next dev",
        "build:next": "next build",
        "dev:electron-next": "concurrently \"yarn dev:next\" \"wait-on http://localhost:3000 && yarn dev:electron\"",
        "build": "yarn run build:next && yarn run build:electron"
    },
    "dependencies": {
        "next": "11.0.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "electron": "16.0.5",
        "@chakra-ui/react": "^1.6.4",
        "@emotion/react": "^11",
        "@emotion/styled": "^11",
        "@material-ui/core": "^4.12.3",
        "@material-ui/icons": "^4.11.2",
        "axios": "^0.24.0",
        "framer-motion": "^4",
        "mysql2": "^2.3.3",
        "validator": "^13.7.0",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@types/mysql": "types/mysql",
        "@types/validator": "^13.7.0",
        "@types/yup": "^0.29.13",
        "@babel/core": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@types/node": "17.0.7",
        "@types/react": "17.0.38",
        "babel-loader": "^8.2.3",
        "concurrently": "^7.0.0",
        "cross-env": "^7.0.3",
        "eslint": "8.6.0",
        "eslint-config-next": "12.0.7",
        "typescript": "4.5.4",
        "wait-on": "^6.0.0",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1"
    }
}